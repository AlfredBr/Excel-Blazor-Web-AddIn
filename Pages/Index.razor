@page "/"

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />

<div class="alert alert-info mt-4"><strong>OfficeContext: </strong>@(OfficeContext ?? "Browser")</div>

<button type="button" class="btn btn-success shadow-none" @onclick="HelloWorld">Hello World</button>

@code {
    private async Task HelloWorld()
    {
        await JSModule.InvokeVoidAsync("helloWorldAsync");
    }
}
